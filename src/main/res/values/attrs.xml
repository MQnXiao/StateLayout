<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StateLayout">

        <attr name="content_layout" format="reference" />
        <attr name="empty_layout" format="reference" />
        <attr name="loading_layout" format="reference" />
        <attr name="error_layout" format="reference" />
        <attr name="defaultState" format="enum">
            <enum name="content" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
            <enum name="error" value="4" />
        </attr>
    </declare-styleable>

</resources>